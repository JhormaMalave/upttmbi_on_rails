<p id="notice"><%= notice %></p>

<h1>Staff Teacher Careers</h1>

<table>
  <thead>
    <tr>
      <th>Teacher</th>
      <th>Career</th>
      <th>Observation</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teacher_careers.each do |teacher_career| %>
      <tr>
        <td><%= teacher_career.teacher_id %></td>
        <td><%= teacher_career.career_id %></td>
        <td><%= teacher_career.observation %></td>
        <td><%= teacher_career.status %></td>
        <td><%= link_to 'Show', staff_teacher_career_path(teacher_career) %></td>
        <td><%= link_to 'Edit', edit_staff_teacher_career_path(teacher_career) %></td>
        <td><%= link_to 'Destroy', staff_teacher_career_path(teacher_career), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Staff Teacher Career', new_staff_teacher_career_path %>
