<!-- Header -->
<div class="header bg-primary pb-6">
  <div class="container-fluid">
    <div class="header-body">
      <div class="row align-items-center py-4">
        <div class="col-lg-6 col-7">
          <nav aria-label="breadcrumb" class="d-none d-md-inline-block ml-md-4">
            <ol class="breadcrumb breadcrumb-links breadcrumb-dark">
              <li class="breadcrumb-item">
                <%= link_to root_path do %>
                  <i class="fas fa-home"></i>
                <% end %>
              </li>
              <li class="breadcrumb-item">
                <%= link_to root_path do %>
                  Inicio
                <% end %>
              </li>
              <li class="breadcrumb-item">
                <%= link_to staff_root_path do %>
                  Panel administrativo
                <% end %>
              </li>
              <li class="breadcrumb-item">
                <%= link_to staff_teacher_degrees_path do %>
                  Docentes titulos
                <% end %>
              </li>
              <li class="breadcrumb-item">
                <%= link_to staff_teacher_degree_path(@teacher_degree) do %>
                  <%= @teacher_degree.name %>
                <% end %>
              </li>
              <li class="breadcrumb-item active" aria-current="page">Editar</li>
            </ol>
          </nav>
        </div>
        <div class="col-lg-6 col-5 text-right">
          <%= link_to 'Nueva', new_staff_teacher_degree_path, class: "btn btn-neutral" %>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="container-fluid mt--6">
    <section class="card">
        <div class="card-header border-0">
            <h2 class="mb-0" >Editar docente</h2>
        </div>
        <div class="px-4">
            <%= render 'form', teacher_degree: @teacher_degree %>
        </div>
    </section>
</div>