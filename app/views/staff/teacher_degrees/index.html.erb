<h1>Staff Teacher Degrees</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Degree type</th>
      <th>Status</th>
      <th>Teacher</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @teacher_degrees.each do |teacher_degree| %>
      <tr>
        <td><%= teacher_degree.name %></td>
        <td><%= teacher_degree.degree_type %></td>
        <td><%= teacher_degree.status %></td>
        <td><%= teacher_degree.teacher_id %></td>
        <td><%= link_to 'Show', staff_teacher_degree_path(teacher_degree) %></td>
        <td><%= link_to 'Edit', edit_staff_teacher_degree_path(teacher_degree) %></td>
        <td><%= link_to 'Destroy', staff_teacher_degree_path(teacher_degree), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Staff Teacher Degree', new_staff_teacher_degree_path %>
